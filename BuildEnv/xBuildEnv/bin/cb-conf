#!/usr/bin/env ruby

require 'nokogiri'
require 'docopt'
require 'fileutils'


class CodeBlocksConfTool
  attr_reader :user_conf_filename, :proj_conf_filename, :extra_conf_filename

  def initialize( user_conf, proj_conf, extra_conf, force_use_default = false, no_merge = false )
    @user_conf_filename  = user_conf
    @proj_conf_filename  = proj_conf
    @extra_conf_filename = extra_conf

    @proj_conf_doc = nil

    createConfFile( force_use_default, no_merge )
  end

  #***************************************************************************************************
  #   \brief Lists the xml sections given
  #***************************************************************************************************
  def listXml( cmd )

    xml_doc = Nokogiri::XML( '<?xml version="1.0" encoding="UTF-8" standalone="yes" ?><CodeBlocksConfig version="1">' )

    case cmd
    when 'user_sets'
      if @proj_conf_doc.at_xpath("//compiler/user_sets")
        xml_doc.root.add_child( Nokogiri::XML::Node.new("compiler", xml_doc) )
        xml_doc.at_xpath("//compiler").add_child( Nokogiri::XML::Node.new("user_sets", xml_doc) )
        xml_doc.at_xpath("//compiler/user_sets").replace @proj_conf_doc.at_xpath("//compiler/user_sets")
      end
    when 'tools'
      if @proj_conf_doc.at_xpath("//ShellExtensions/shellcmds")
        xml_doc.root.add_child( Nokogiri::XML::Node.new("ShellExtensions", xml_doc) )
        xml_doc.at_xpath("//ShellExtensions").add_child( Nokogiri::XML::Node.new("shellcmds", xml_doc) )
        xml_doc.at_xpath("//ShellExtensions/shellcmds").replace @proj_conf_doc.at_xpath("//ShellExtensions/shellcmds")
      end
    when 'all'

      if @proj_conf_doc.at_xpath("//compiler/user_sets")
        xml_doc.root.add_child( Nokogiri::XML::Node.new("compiler", xml_doc) )
        xml_doc.at_xpath("//compiler").add_child( Nokogiri::XML::Node.new("user_sets", xml_doc) )
        xml_doc.at_xpath("//compiler/user_sets").replace @proj_conf_doc.at_xpath("//compiler/user_sets")
      end

      if @proj_conf_doc.at_xpath("//ShellExtensions/shellcmds")
        xml_doc.root.add_child( Nokogiri::XML::Node.new("ShellExtensions", xml_doc) )
        xml_doc.at_xpath("//ShellExtensions").add_child( Nokogiri::XML::Node.new("shellcmds", xml_doc) )
        xml_doc.at_xpath("//ShellExtensions/shellcmds").replace @proj_conf_doc.at_xpath("//ShellExtensions/shellcmds")
      end

    else
      puts "Error: unknown list_xml sub-command"
      exit 1
    end

    puts xml_doc
  end

  #***************************************************************************************************
  #   \brief Writes the project file to disk
  #***************************************************************************************************
  def write_to( fname )
    File.open(fname, 'w') do |file|
      @proj_conf_doc.write_xml_to file
    end
  end

  #***************************************************************************************************
  #   \brief Resets the users default.conf to the internal supplied by this tool
  #***************************************************************************************************
  def resetUserDefaultConf
    if File.exists?( @user_conf_filename )
      FileUtils.cp( @user_conf_filename, "#{@user_conf_filename}.bak")
    end

    # Use the built-in default
    int_conf_doc = Nokogiri::XML( SEL_CONF_XML )

    File.open(@user_conf_filename, 'w') do |file|
      int_conf_doc.write_xml_to file
    end
  end

  private
  #***************************************************************************************************
  #   \brief Creates project specific
  #***************************************************************************************************
  def createConfFile( force_use_default = false, no_merge = false )

    # Check for local_conf first and use it if possible
    unless File.exists?( @proj_conf_filename ) or force_use_default

      # Copy over the user's own file if it exists
      if File.exists?( @user_conf_filename )
        FileUtils.cp( @user_conf_filename, @proj_conf_filename )
      else
        # Otherwise use the built-in default
        int_conf_doc = Nokogiri::XML( SEL_CONF_XML )

        File.open(@proj_conf_filename, 'w') do |file|
          int_conf_doc.write_xml_to file
        end
      end
    end #unless proj_conf_filename exists


    ### Okay - now the file should definately exist in some form so we can merge in our user_sets
    @proj_conf_doc = Nokogiri::XML( File.read( @proj_conf_filename ) )
    user_sets_doc  = Nokogiri::XML( SEL_USER_SETS_XML )


    ### Are we merging
    return if no_merge

    @proj_conf_doc.at_xpath("//compiler").add_child( Nokogiri::XML::Node.new("user_sets", @proj_conf_doc) ) unless @proj_conf_doc.at_xpath "//compiler/user_sets"
    @proj_conf_doc.at_xpath("//compiler/user_sets").replace user_sets_doc.at_xpath("//compiler/user_sets")


    ### Toast the recent file and project lists
    @proj_conf_doc.xpath("RECENT_FILES").each     { |element| element.delete('*') }
    @proj_conf_doc.xpath("RECENT_PROJECTS").each  { |element| element.delete('*') }


    ### Merge in the project specific tools if they exist
    if File.exists?( @extra_conf_filename )
      extra_conf_doc = Nokogiri::XML( File.read( @extra_conf_filename ) )

      if extra_conf_doc.at_xpath("//ShellExtensions/shellcmds")
        @proj_conf_doc.at_xpath("//ShellExtensions").add_child( Nokogiri::XML::Node.new("shellcmds", @proj_conf_doc) ) unless @proj_conf_doc.at_xpath "//ShellExtensions/shellcmds"
        @proj_conf_doc.at_xpath("//ShellExtensions/shellcmds").replace extra_conf_doc.at_xpath("//ShellExtensions/shellcmds")
      end

      ## Now any additional/overrides from user_sets
      if extra_conf_doc.at_xpath("//compiler/user_sets")
        extra_conf_doc.at_xpath("//compiler/user_sets").elements.each do |compiler|

          if @proj_conf_doc.at_xpath( "//compiler/user_sets/#{compiler.name}" )
            @proj_conf_doc.at_xpath( "//compiler/user_sets/#{compiler.name}" ).replace compiler
          else
            @proj_conf_doc.at_xpath( "//compiler/user_sets" ).add_child compiler
          end
        end
      end
    end
  end #createConfFile

end # class CodeBlocksConfTool



# Grab just the filename for convenience
NAME = File.basename($0)

# Version information string
VERSION_NUM = "1.0"
VERSION = <<END_VERSION
########################################################################
#  #{NAME} #{VERSION_NUM}
#  Tool for manipulating the CodeBlocks default.conf file
#  Copyright (C) 2014 Schrader Electronics
########################################################################
END_VERSION

# Build the options!  Notice that #{VERSION} will include a newline at end
DOC_OPT = <<DOCOPT
#{VERSION}
Usage:
  #{NAME} [--force-default]
  #{NAME} list_xml (user_sets|tools|all)
  #{NAME} make_default
  #{NAME} reset_default
  #{NAME} cleanup
  #{NAME} -h | --help
  #{NAME} --version

Options:
  --version           Show version info.
  -h --help           Show this screen.
  -f --force-default  Forces the use of the users default.conf (if it exists)
DOCOPT

#***************************************************************************************************
#   \brief          Main Program
#***************************************************************************************************
# Command Line Support ###############################
if ($0 == __FILE__)

  # Extract options!
  options = {}
  begin
    options = Docopt::docopt(DOC_OPT)
  rescue Docopt::Exit => e
    puts e.message
    exit 1
  end

  user_conf_filename  = "#{ENV['APPDATA']}/CodeBlocks/default.conf"
  proj_conf_filename  = "#{ENV['APPDATA']}/CodeBlocks/#{ENV['PROJ_TITLE']}.conf"
  extra_conf_filename = "#{ENV['PBCD']}/CodeBlocks/proj.conf"


  if options['list_xml']
    tool = CodeBlocksConfTool.new( user_conf_filename, proj_conf_filename, extra_conf_filename, false, true )
    tool.listXml( 'user_sets' ) if options['user_sets']
    tool.listXml( 'tools' )     if options['tools']
    tool.listXml( 'all' )       if options['all']

  elsif options['make_default']
    tool = CodeBlocksConfTool.new( user_conf_filename, proj_conf_filename, extra_conf_filename )
    tool.write_to( user_conf_filename )

  elsif options['reset_default']
    tool = CodeBlocksConfTool.new( user_conf_filename, proj_conf_filename, extra_conf_filename )
    tool.resetUserDefaultConf()

  elsif options['cleanup']
    Dir.chdir ("#{ENV['APPDATA']}/CodeBlocks/") do
      FileUtils.rm_f Dir.glob("#{ENV['PROJ_TITLE']}.*")
    end

  else
    tool = CodeBlocksConfTool.new( user_conf_filename, proj_conf_filename, extra_conf_filename, options['--force-default'] )
    tool.write_to( proj_conf_filename )
  end

  exit 0

end #if ($0 == __FILE__)



